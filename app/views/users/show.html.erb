<h1>Username: <%= @user.username %></h1><br>
<h4>These are your actions to date. Click on each to update.</h4><br>
  <!-- add ability to edit here -->

<% @user.actions.each do |action| %>
<p>
  <ul>
    <li>Action #: <%= action.id %></li>
    <li>Action Name: <%= action.action_name %></li>
    <li>Action Status: <%= action.status %></li>
    <li><a href="/actions/:id/edit">Edit this action</a></li>
  </ul>
</p>
<% end %>

<h5><a href="/users">Return to menu</a></h5>
