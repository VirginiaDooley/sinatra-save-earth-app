<h1>Username: <%= current_user.username %></h1><br>
<h4>These are your actions to date.</h4><br>

<% current_user.actions.each do |action| %>
<p>
  <ul>
    <li><a href="/actions/<%= action.id %>">Action #<%= action.id %></a></li>
    <li>Action Name: <%= action.action_name %></li>
    <li>Action Status: <%= action.status %></li>
  </ul>
</p>
<% end %>

<h5><a href="/users">Return to main menu</a></h5>
